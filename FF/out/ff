#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x100cb7160 .scope module, "ff_tb" "ff_tb" 2 4;
 .timescale -9 -12;
v0x100cbc0d0_0 .var "clk", 0 0;
v0x100cbc170_0 .var "d", 0 0;
v0x100cbc210_0 .net "q", 0 0, v0x100cbbf90_0;  1 drivers
v0x100cbc2b0_0 .var "rst", 0 0;
S_0x100cb4bc0 .scope module, "dut" "ff" 2 12, 3 4 0, S_0x100cb7160;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
v0x100cb4d40_0 .net "clk", 0 0, v0x100cbc0d0_0;  1 drivers
v0x100cb4de0_0 .net "d", 0 0, v0x100cbc170_0;  1 drivers
v0x100cbbf90_0 .var "q", 0 0;
v0x100cbc030_0 .net "rst", 0 0, v0x100cbc2b0_0;  1 drivers
E_0x100cb78c0 .event posedge, v0x100cbc030_0, v0x100cb4d40_0;
    .scope S_0x100cb4bc0;
T_0 ;
    %wait E_0x100cb78c0;
    %load/vec4 v0x100cbc030_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x100cbbf90_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x100cb4de0_0;
    %assign/vec4 v0x100cbbf90_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x100cb7160;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x100cbc0d0_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x100cbc0d0_0;
    %inv;
    %store/vec4 v0x100cbc0d0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x100cb7160;
T_2 ;
    %vpi_call 2 27 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x100cb7160 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x100cb7160;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x100cbc2b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x100cbc170_0, 0, 1;
    %delay 12000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x100cbc2b0_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x100cbc170_0, 0, 1;
    %delay 20000, 0;
    %delay 3000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x100cbc2b0_0, 0, 1;
    %delay 4000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x100cbc2b0_0, 0, 1;
    %delay 2000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x100cbc170_0, 0, 1;
    %delay 20000, 0;
    %vpi_call 2 55 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
